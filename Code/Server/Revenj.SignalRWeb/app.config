<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="autofacConfiguration" type="Autofac.Configuration.SectionHandler, Autofac.Configuration" />
  </configSections>
  <appSettings>
    <add key="Listen" value="http://127.0.0.1:8998" />
    <add key="ServerAssembly" value="../cache/GeneratedModel.dll" />
    <add key="SignalR.CrossDomain" value="true" />
    <add key="ConnectionString" value="server=localhost;port=5432;database=dbname;user=dbuser;password=dbpass;encoding=unicode" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0">
    </compilation>
    <customErrors mode="Off" />
  </system.web>
  <autofacConfiguration>
    <modules>
      <module type="Revenj.Wcf.StandardModule, Revenj.Wcf" />
    </modules>
  </autofacConfiguration>
</configuration>
